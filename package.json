{
    "version": "0.1.4",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {},
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "bracketSpacing": true
    },
    "name": "neo4ts",
    "author": "kingskull619",
    "module": "dist/neo4ts.esm.js",
    "size-limit": [
        {
            "path": "dist/neo4ts.cjs.production.min.js",
            "limit": "1024 KB"
        },
        {
            "path": "dist/neo4ts.esm.js",
            "limit": "1024 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.1",
        "husky": "^7.0.1",
        "size-limit": "^5.0.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "neo4j-driver": "^4.3.1"
    },
    "resolutions": {
        "**/@typescript-eslint/eslint-plugin": "^4.1.1",
        "**/@typescript-eslint/parser": "^4.1.1"
    },
    "keywords": [
        "neo4j",
        "graph",
        "database",
        "query",
        "cypher",
        "model",
        "datastore",
        "nosql",
        "orm",
        "ogm",
        "db"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/kingskull619/Neo4Ts.git"
    }
}
